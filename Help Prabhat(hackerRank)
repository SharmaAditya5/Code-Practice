//program to convert lower case letter to upper case
#include<ctype.h>
#include<string.h>
#include<stdio.h>
#include<conio.h>


int main() {
    char c[103];
    int i;
    gets(c);
   for(i=0;c[i]!='\0';i++)
   {
       if(c[i]>='a' && c[i]<='z')
           c[i]=c[i]-32;
   }
    puts(c);
    return 0;
}
